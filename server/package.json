{
  "name": "edumeet-server",
  "version": "3.5.8",
  "private": true,
  "description": "edumeet server",
  "author": "Håvar Aambø Fosstveit <h@fosstveit.net>",
  "contributors": [
    "Stefan Otto",
    "Mészáros Mihály",
    "Roman Drozd",
    "Rémai Gábor László",
    "Piotr Pawałowski"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "scripts": {
    "start": "node dist/server.js",
    "build": "mkdir -p dist && touch dist/tmp && find dist/* -maxdepth 0 ! -name public -exec rm -rf {} \\; && tsc && ln -s ../certs dist/certs && chmod 755 dist/server.js && ( for fileExt in yaml json toml ; do [ -f config/config.$fileExt ] && cp config/config.$fileExt dist/config/; done ) | true && touch 'dist/ __AUTO_GENERATED_CONTENT_REFRESHED_AFTER_REBUILDING!__ '",
    "dev": "nodemon --exec ts-node --ignore dist/ -e js,ts server.js",
    "connect": "ts-node connect.js",
    "lint": "eslint ./ --ext .js,.ts; exit 0",
    "lint-fix": "eslint ./ --fix --ext .js,.ts; exit 0",
    "gen-config-docs": "ts-node utils/gen-config-docs.ts"
  },
  "dependencies": {
    "awaitqueue": "3.0.2",
    "axios": "1.12.0",
    "base-64": "1.0.0",
    "bcrypt": "5.1.1",
    "body-parser": "1.20.3",
    "colors": "1.4.0",
    "compression": "1.8.1",
    "connect-redis": "8.0.1",
    "convict": "6.2.4",
    "convict-format-with-validator": "6.2.0",
    "cookie-parser": "1.4.7",
    "debug": "4.4.0",
    "express": "4.21.2",
    "express-session": "1.18.2",
    "express-socket.io-session": "1.3.5",
    "fast-stats": "0.0.7",
    "helmet": "8.0.0",
    "json5": "2.2.3",
    "jsonwebtoken": "9.0.2",
    "mediasoup": "3.19.2",
    "openid-client": "5.7.1",
    "os-utils": "0.0.14",
    "passport": "0.7.0",
    "passport-local": "1.0.0",
    "@node-saml/passport-saml": "5.1.0",
    "pidusage": "3.0.2",
    "prom-client": "15.1.3",
    "redis": "4.7.0",
    "socket.io": "4.8.1",
    "spdy": "4.0.2",
    "toml": "3.0.0",
    "uuid": "11.0.5",
    "yaml": "2.7.0"
  },
  "resolutions": {
    "express-socket.io-session/cookie-parser": "1.4.7",
    "brace-expansion": "2.0.2",
